[General]
arch=arm64
unpack=true
noauth=false
bootstrap=Debian Net Utils
aptsources=Debian

[Debian]
packages=apt systemd
source=http://http.debian.net/debian
keyring=debian-archive-keyring
suite=stable

[Net]
#Basic packages to enable the networking
packages=netbase net-tools ethtool udev iproute2 iputils-ping ifupdown isc-dhcp-client
# "ssh" package is conflicting with "openssh-server"
source=http://http.debian.net/debian

[Utils]
packages=locales adduser nano less wget dialog usbutils tmux sudo git
source=http://http.debian.net/debian
